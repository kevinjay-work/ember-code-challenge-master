<table class="table">
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Company</th>
  <th>Project</th>
  <th>Budget</th>
  <th>Actions: </th>

  {{#each this.model as |row index|}}
  <tr>
    <td>{{row.firstName}}</td>
    <td>{{row.lastName}}</td>
    <td>{{row.emailAddress}}</td>
    <td>{{row.company}}</td>
    <td>{{row.project}}</td>
    <td>{{row.budget}}</td>
    <td>
      <a {{action "onRowClick" row.id}}>Edit</a>


    </td>

  </tr>
  {{/each}}
</table>