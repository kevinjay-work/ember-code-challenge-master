<table class="table">
  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email Address</th>
    <th>Phone Number</th>
    <th>Company Name</th>
    <th>Project</th>
    <th>Budget</th>
    <th>Actions: </th>
  </thead>
  {{#each this.model as |row|}}
    <tbody>
      <tr>
        <td>{{row.firstName}}</td>
        <td>{{row.lastName}}</td>
        <td>{{row.emailAddress}}</td>
        <td>{{row.phoneNumber}}</td>
        <td>{{row.company}}</td>
        <td>{{row.project}}</td>
        <td>{{row.budget}}</td>
        <td>
          <a href=""{{action "onRowClick" row.id}}>Edit</a>
        </td>
      </tr>
    </tbody>
  {{/each}}
</table>